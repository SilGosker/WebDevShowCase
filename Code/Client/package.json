{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --port 53782",
    "check": "svelte-check",
    "tailwind:build": "tailwindcss -i ./src/app.css -o ./public/build/tailwind.css",
    "tailwind:watch": "tailwindcss -i ./src/app.css -o ./public/build/tailwind.css --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.1.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "@glidejs/glide": "^3.7.1",
    "chart.js": "^4.4.7",
    "sirv-cli": "^2.0.0",
    "svelte-recaptcha-v2": "^0.0.2",
    "svelte-spa-router": "^4.0.1",
    "sweetalert2": "^11.16.0"
  }
}
